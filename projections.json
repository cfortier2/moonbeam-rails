{
  "app/assets/javascripts/router.js": {
    "command": "jini"
  },
  "app/controllers/api/v1/*_controller.rb": {
    "command": "apicontroller",
    "alternate": "spec/controllers/api/v1/%s_controller_spec.rb",
    "template": "apicontroller"
  },
  "app/serializers/*_serializer.rb": {
    "command": "serializers",
    "template": "serializer"
  },
  "app/assets/javascripts/models/*.js": {
    "command": "jmodel",
    "alternate": "spec/javascripts/models/%s_spec.js",
    "template": "jmodel"
  },
  "app/assets/javascripts/views/*.js": {
    "command": "jview",
    "alternate": "spec/javascripts/views/%s_spec.js",
    "template": "jview"
  },
  "app/assets/javascripts/controllers/*.js": {
    "command": "jcontroller",
    "alternate": "spec/javascripts/controllers/%s_spec.js",
    "template": "jcontroller"
  },
  "app/assets/javascripts/routes/*.js": {
    "command": "jroute",
    "alternate": "spec/javascripts/routes/%s_spec.js",
    "template": "jroute"
  },
  "app/assets/javascripts/mixins/*.js": {
    "command": "jmixin",
    "alternate": "spec/javascripts/mixins/%s_spec.js",
    "template": "jmixin"
  },
  "app/assets/javascripts/templates/*.hbs": {
    "command": "jtemplate",
    "template": "blank"
  },
 "spec/javascripts/**/*_spec.js": {
    "command": "jspec",
    "alternate": "app/assets/javascripts/%s.js"
  },

  "templates": {
    "apicontroller": "class Api::V1::%SController < ApplicationController\n  respond_to :json\nend",
    "serializer": "class %SSerializer < ActiveModel::Serializer\nend",
    "jmodel": "App.%S = DS.Model.extend({\n});",
    "jview": "App.%SView = Ember.View.extend({\n});",
    "jcontroller": "App.%SController = Ember.Controller.extend({\n});",
    "jroute": "App.%SRoute = Ember.Route.extend({\n});",
    "jmixin": "App.%S = Ember.Mixin.create({\n});",
    "blank": ""
  }
}
